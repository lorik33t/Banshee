{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-exists",
    "fs:allow-read-dir",
    {
      "identifier": "fs:scope",
      "allow": ["**"]
    }
  ]
}
